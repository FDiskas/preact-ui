{
  "name": "altv-ui",
  "version": "0.0.1",
  "license": "MIT",
  "description": "UI for the altv game",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "rimraf dist && parcel build src/index.html --target node --bundle-node-modules --no-source-maps --experimental-scope-hoisting",
    "preserve": "yarn build",
    "serve": "serve dist -s -n",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\""
  },
  "dependencies": {
    "preact": "^10.5.7",
    "preact-router": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-plugin-prettier": "^3.2.0",
    "parcel-bundler": "^1.12.4",
    "postcss": "^8.2.1",
    "postcss-modules": "^3.2.2",
    "preact-cli": "^3.0.5",
    "preact-render-to-string": "*",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "typescript": "^4.1.2"
  }
}
